NODE_ENV=production
PORT=5000

# Database
MONGO_URI=mongodb://mongo:27017/health-prediction

# JWT
JWT_SECRET=${JWT_SECRET}
JWT_EXPIRE=7d
JWT_COOKIE_EXPIRE=7

# Email
EMAIL_HOST=${EMAIL_HOST}
EMAIL_PORT=${EMAIL_PORT}
EMAIL_USERNAME=${EMAIL_USERNAME}
EMAIL_PASSWORD=${EMAIL_PASSWORD}
EMAIL_FROM=noreply@healthprediction.rw

# ML Service API
ML_API_URL=https://ml-api.healthprediction.rw
ML_API_KEY=${ML_API_KEY}

# Frontend URL
FRONTEND_URL=https://healthprediction.rw

# Log Level (error, warn, info, debug)
LOG_LEVEL=error
